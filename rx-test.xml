<?xml version="1.0"?>
<prescriptions xmlns="http://greenify.github.io/schemas/prescription/v3">
  <!-- 腾讯手机管家 -->
  <prescription type="activity" package="com.tencent.qqpimsecure" class="com.tencent.server.fore.DefaultSmsQuickLoadActivity" sender="other">
    <intent-filter>
      <action name="android.intent.action.SEND"/>
      <action name="android.intent.action.SENDTO"/>
      <cat name="android.intent.category.DEFAULT"/>
      <cat name="android.intent.category.BROWSABLE"/>
      <scheme name="sms"/>
      <scheme name="smsto"/>
      <scheme name="mms"/>
      <scheme name="mmsto"/>
    </intent-filter>
  </prescription>
</prescriptions>
